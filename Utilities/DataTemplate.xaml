<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:HurtowniaAplikacja">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <vm:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomerVM}">
        <vm:Customers />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <vm:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:OrderVM}">
        <vm:Orders />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <vm:Settings />
    </DataTemplate>

</ResourceDictionary>